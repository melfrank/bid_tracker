        <name>HELP - bid date 2 weeks from current</name>
        <order/>
        <script><![CDATA[function onCellEdit(sysIDs, table, oldValues, newValue, callback) {
  var saveAndClose = true;
	
 /*
Purpose:  This client script will prevent a user from selecting a "due date" that is less than 2 weeks from current date.  An error message will appear below the due date field, and the due date field will clear.
*/
	
 if (isLoading || newValue === '') {
      return;
   }
	//get start date
	var start = g_form.getValue('original_bid_date');
	//get Date object using user's display format
	var dt = getDateFromFormat(start, g_user_date_time_format);
	

	
var rightNow = new Date();
	
	if (dt < rightNow) {
		g_form.clearValue('original_bid_date');
		g_form.showFieldMsg('original_bid_date','Due date must be in the future.','error');
	}
	
 
 callback(saveAndClose); 
}]]></script>
